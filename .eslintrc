{
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended"
  ],
  "plugins": [
    "react",
    "standard"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": 0
  }
}